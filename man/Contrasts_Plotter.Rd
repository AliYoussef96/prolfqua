cp$histogram_estimate()
res <- cp$volcano()
length(res)
res
respltly <- cp$volcano_plotly()
length(respltly)
cp$ma_plot()
cp$ma_plotly
if(FALSE){
  cp$write_pdf("c:/Temp")
